<!DOCTYPE html>
<html lang="en">
<%- include('./partials/header.ejs')  %> 
<%- include('./partials/nav.ejs')  %> 
<body>
    <table class="table table-striped table-hover">
        <thead>
          <tr style="background-color: #cf6034;">
            <th scope="col">#</th>
            <th scope="col">Circle</th>
            <th scope="col">Forest Division</th>
            <th scope="col">Plantation Year</th>
            <th scope="col">Total Sites</th>
            <th scope="col">Plantation Area(Ac)</th>

          </tr>
        </thead>
        <tbody>
          <% var count = 0 %>
          <% summary.forEach((data)=>{%>
            
            <tr>
                <% count = count +1%>
                <th scope="row"><%=count  %> </th>
                <td><%=data.f_circle %></td>
                <td><%=data.f_division %></td>
                <td><%=data.p_year %></td>
                <td id = 'sitesum'><%=data.total_sites %></td>
                <td><%=data.area_acres %></td>
                
              </tr>
            <!-- <p><%=data.f_circle %> </p> -->
          <%})%> 

          <tr style="background-color: #cf6034;">
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td style="font-weight:bold" ><%=total_num.total_sites%></td>
            <td style="font-weight:bold"><%=total_num.total_area %> </td>

          </tr>

        </tbody>
      </table>
</body>
</html>